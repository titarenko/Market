@model IEnumerable<Market.Cqrsnes.Projection.User>

@{
    ViewBag.Title = "Users";
}

<h2>Users</h2>

<table>
    <thead>
        <tr>
            <th>Name</th>
            <th>Balance</th>
        </tr>
    </thead>
    <tbody>
    @foreach(var user in Model)
    {
        <tr>
            <td>@user.Name</td>
            <td>@user.Balance</td>
        </tr>
    }
    </tbody>
</table>
